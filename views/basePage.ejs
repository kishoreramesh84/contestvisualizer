<!doctype html>
<html lang="en">
  <head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" integrity="sha384-JcKb8q3iqJ61gNV9KGb8thSsNjpSL0n8PARn9HuZOnIxN0hoP+VmmDGMN5t9UJ0Z" crossorigin="anonymous">

    <title>Contest Hub | <%= page_name %></title>

    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-177313454-1"></script>
    <script>
      window.dataLayer = window.dataLayer || [];
      function gtag(){dataLayer.push(arguments);}
      gtag('js', new Date());

      gtag('config', 'UA-177313454-1');
    </script>
    <!-- Google Analytics -->


  </head>
  <style type="text/css">
    table th{
      text-align: center;
      background-color: #F5F5F5;
    }
    table td{
      text-align: center;
    }
    .btn-warning{
      color:white;
    }
    tr:hover{
      background-color: #b0e0e6;
    }
    #crct{
      text-decoration: none;
      color:black;
    }
  </style>

  <body>

    <!-- Header Part -->
    <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
      <a class="navbar-brand" href="/">Contest Hub</a>
      <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavDropdown" aria-controls="navbarNavDropdown" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="container">
        <div class="collapse navbar-collapse" id="navbarNavDropdown">
          <ul class="navbar-nav">
            <li class="nav-item <% if (page_name === 'Leetcode') { %> active <% } %>">
              <a class="nav-link" href="/">
                <img src="https://leetcode.com/static/images/LeetCode_logo.png" width="30" height="30" class="d-inline-block align-top" alt="" loading="lazy">LeetCode</a>
            </li>
            <li class="nav-item <% if (page_name === 'Codeforces') { %> active <% } %>">
              <a class="nav-link" href="/codeforces">
                <img src="https://is3-ssl.mzstatic.com/image/thumb/Purple123/v4/40/e8/23/40e823e3-440c-00d3-9df2-e92c75abe447/AppIcon-0-0-1x_U007emarketing-0-0-0-10-0-0-sRGB-0-0-0-GLES2_U002c0-512MB-85-220-0-0.png/246x0w.png" width="30" height="30" class="d-inline-block align-top" alt="" loading="lazy"> Codeforces</a>
            </li>
            <li class="nav-item <% if (page_name === 'Hackerearth') { %> active <% } %>">
              <a class="nav-link" href="/hackerearth">
                <img src="https://avatars3.githubusercontent.com/u/3033794?s=280&v=4" height="30" width="30" class="d-inline-block align-top" loading="lazy"> Hackerearth</a>
            </li>
            <li class="nav-item <% if (page_name === 'Codechef') { %> active <% } %>">
              <a class="nav-link" href="/codechef">
                <img src="https://i.pinimg.com/originals/c5/d9/fc/c5d9fc1e18bcf039f464c2ab6cfb3eb6.jpg" height="30" width="30" class="d-inline-block align-top" loading="lazy"> CodeChef</a>
            </li>
            <li class="nav-item <% if (page_name === 'Hackerrank') { %> active <% } %>">
              <a class="nav-link" href="/hackerrank">
                <img src="https://upload.wikimedia.org/wikipedia/commons/4/40/HackerRank_Icon-1000px.png" height="30" width="30" class="d-inline-block align-top" loading="lazy"> HackerRank</a>
            </li>
            <li class="nav-item <% if (page_name === 'Atcoder') { %> active <% } %>">
              <a class="nav-link" href="/atcoder">
                <img src="https://img.atcoder.jp/assets/icon/avatar.png" height="30" width="30" class="d-inline-block align-top" loading="lazy"> AtCoder</a>
            </li>
          </ul>
        </div>
      </div>
    </nav>
    <!-- Header Part end -->

    <div class="container">
      <p></p>
      <h1>Active Contests</h1> 
      <hr></hr>
      <% if(AC.length===0){ %>
        <p>No Contest Currently Avaliable </p>
      <% }else{ %>
      <table class="table table-bordered table-responsive">
        <thead>
          <tr>
            <th>S.No</th>
            <th>Event Name</th>
            <th>Start Time</th>
            <th>End Time</th>
            <th>Duration</th>
            <th>Action</th>
          </tr>
        </thead>
        <tbody>
          <% for(var i=0;i<AC.length;i++){ %>
          <tr>
          <td id="snoAC"><%- i+1 %></td>
          <td><a href="<%= AC[i].href %>" target="_blank" id="crct"><%= AC[i].event %></a></td>
          <td><%= AC[i].start %></td>
          <td><%= AC[i].end %></td> 
          <td><%= AC[i].duration %></td>
          <td><a href="<%= AC[i].href %>" target="_blank" class="btn btn-warning" role="button" aria-pressed="true">Join Now</a></td>
          <% } %>
          </tr>
        </tbody>
      </table>
      <% } %>

      <h1>Upcoming Contests</h1> 
      <hr></hr>
      <% if(UC.length==0){ %>
        <p>No Upcoming Contest Avaliable</p>
      <% }else{ %>
      <table class="table table-bordered table-responsive">
        <thead>
          <tr>
            <th>S.No</th>
            <th>Event Name</th>
            <th>Start Time</th>
            <th>End Time</th>
            <th>Duration</th>
            <th>Action</th>
          </tr>
        </thead>
        <tbody>
          <% for(var i=0;i<UC.length;i++){ %>
          <tr>
          <td id="snoUC"><%- i+1 %></td>
          <td><a href="<%= UC[i].href %>" target="_blank" id="crct"><%= UC[i].event %></a></td>
          <td><%= UC[i].start %></td>
          <td><%= UC[i].end %></td>
          <td><%= UC[i].duration %></td>
          <td><a href="<%= UC[i].href %>" target="_blank" class="btn btn-success" role="button" aria-pressed="true">Register</a></td>
          <% } %>
          </tr>
        </tbody>
      </table>
      <% } %>

      <h1>Past Contests</h1> 
      <hr></hr>
      <% if(PC.length==0){ %>
        <p>No Contest Currently Avaliable </p>
      <% }else{ %>
      <table class="table table-bordered table-responsive">
        <thead>
          <tr>
            <th>S.No</th>
            <th>Event Name</th>
            <th>Start Time</th>
            <th>End Time</th>
            <th>Duration</th>
            <th>Action</th>
          </tr>
        </thead>
        <tbody>
          <% for(var i=0;i<PC.length;i++){ %>
          <tr>
          <td id="snoPC"><%- i+1 %></td>
          <td><a href="<%= PC[i].href %>" target="_blank" role="button" id="crct"><%= PC[i].event %></a></td>
          <td><%= PC[i].start %></td>
          <td><%= PC[i].end %></td>
          <td><%= PC[i].duration %></td>
          <td><a href="<%= PC[i].href %>" target="_blank" class="btn btn-info" role="button" aria-pressed="true">View</a></td>
          <% } %>
          </tr>
        </tbody>
      </table>
      <% } %>
    </div>






    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js" integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js" integrity="sha384-9/reFTGAW83EW2RDu2S0VKaIzap3H66lZH81PoYlFhbGU+6BZp6G7niu735Sk7lN" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js" integrity="sha384-B4gt1jrGC7Jh4AgTPSdUtOBvfO8shuf57BaghqFfPlYxofvL8/KUEfYiJOMMV+rV" crossorigin="anonymous"></script>


  </body>
</html>

